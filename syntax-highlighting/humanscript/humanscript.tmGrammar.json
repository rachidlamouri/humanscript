{
  "scopeName": "source.humanscript",
  "patterns": [
    {
      "match": "#.*$",
      "name": "comment.line.number-sign.humanscript"
    },
    {
      "match": "(floor)\\s+(size)",
      "captures": {
        "1": {
          "name": "support.constant.humanscript"
        },
        "2": {
          "name": "keyword.control.humanscript"
        }
      }
    },
    {
      "match": "inbox|outbox|floor",
      "name": "support.constant.humanscript"
    },
    {
      "match": "while|if|else",
      "name": "keyword.control.humanscript"
    },
    {
      "match": "let",
      "name": "storage.type.humanscript"
    },
    {
      "match": "labeled",
      "name": "keyword.control.humanscript"
    },
    {
      "match": "=|\\+|-|!=|==|>|<",
      "name": "keyword.operator.humanscript"
    },
    {
      "match": "[a-z][a-z0-9]*",
      "name": "variable.other.humanscript"
    },
    {
      "match": "\\d+",
      "name": "constant.numeric.humanscript"
    }
  ]
}
