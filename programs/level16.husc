-- # Absolute Positivity
-- #
-- # Send each thing from the
-- # INBOX to the OUTBOX. BUT, if
-- # a number is negative, first
-- # remove its negative sign.

-- FloorInitNode
  -- $accumulator -> floor[1]
  -- $iterator -> null

-- LetStatementNode
  -- tmp -> floor[0]

-- WhileStatementNode
JUMP condition00
loop00:
  -- AssignmentStatementNode
    -- read
    INBOX
    -- write
    COPYTO 0 --$tmp

  -- IfStatementNode
  -- 01
    -- condition
      -- LessThanConditionNode
      COPYFROM 0 --$tmp
      -- compare 0
      JUMPN if01
    else01:
      -- block
      COMMENT 1 -- ## is pos

      -- AssignmentStatementNode
        -- read
        COPYFROM 0 --$tmp
        -- write
        OUTBOX
    JUMP end01
    if01:
      -- block
      COMMENT 0 -- ## negate

      -- AssignmentStatementNode
        -- read
        -- NegationExpressionNode
        COPYFROM 0 --$tmp
        COPYTO 1 --$$accumulator
        SUB 1 --$$accumulator
        SUB 1 --$$accumulator
        -- write
        OUTBOX
  end01:

  condition00:
  JUMP loop00
break00:

