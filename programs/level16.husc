-- # Absolute Positivity
-- # Send each thing from the
-- # INBOX to the OUTBOX. BUT, if
-- # a number is negative, first
-- # remove its negative sign.

-- FloorInitNode
  -- register -> floor[1]

-- LetStatementNode
  -- tmp -> floor[0]

-- WhileStatementNode
A:
  -- AssignmentStatementNode
    -- read
    INBOX
    -- write
    COPYTO 0 --$tmp
  
  -- IfStatementNode
    -- condition
      -- LessThanConditionNode
      COPYFROM 0 --$tmp
      -- compare 0
      JUMPN B
    -- else
    C:
      -- block
      COMMENT 1 -- ## is pos
      
      -- AssignmentStatementNode
        -- read
        COPYFROM 0 --$tmp
        -- write
        OUTBOX
    JUMP D
    -- if
    B:
      -- block
      COMMENT 0 -- ## negate
      
      -- AssignmentStatementNode
        -- read
        -- NegationExpressionNode
        COPYFROM 0 --$tmp
        COPYTO 1 --$register
        SUB 1 --$register
        SUB 1 --$register
        -- write
        OUTBOX
  D:
JUMP A

