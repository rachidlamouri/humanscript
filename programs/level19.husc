-- # Countdown
-- # 
-- # For each number in the INBOX,
-- # send that number to the
-- # OUTBOX, followed by all
-- # numbers down to (or up to)
-- # zero. It's a countdown!
-- # 
-- # You got new commands! They
-- # add ONE or subtract ONE from
-- # an item on the floor. The result
-- # is given back to you, and for
-- # your convenience, also written
-- # right back to the floor. BUMP!

-- FloorInitNode
  -- $accumulator -> null
  -- $iterator -> null

-- LetStatementNode
  -- count -> floor[0]

-- WhileStatementNode
JUMP condition00
loop00:
  -- AssignmentStatementNode
    -- read
    INBOX
    -- write
    COPYTO 0 --$count
  
  -- IfStatementNode
  -- 01
    -- condition
      -- GreaterThanOrEqualToConditionNode
      COPYFROM 0 --$count
      -- compare 0
      JUMPN else01
    if01:
      -- block
      -- WhileStatementNode
      JUMP condition02
      loop02:
        -- AssignmentStatementNode
          -- read
          COPYFROM 0 --$count
          -- write
          OUTBOX
        
        -- DecrementAssignmentExpressionNode
        BUMPDN 0 --$count
        
        condition02:
        -- GreaterThanOrEqualToConditionNode
        COPYFROM 0 --$count
        -- compare 0
        JUMPN break02
        JUMP loop02
      break02:
    JUMP end01
    else01:
      -- block
      -- WhileStatementNode
      JUMP condition03
      loop03:
        -- AssignmentStatementNode
          -- read
          COPYFROM 0 --$count
          -- write
          OUTBOX
        
        -- IncrementAssignmentExpressionNode
        BUMPUP 0 --$count
        
        condition03:
        -- LessThanOrEqualToConditionNode
        COPYFROM 0 --$count
        -- compare 0
        JUMPN loop03
        JUMPZ loop03
      break03:
  end01:
  
  condition00:
  JUMP loop00
break00:

