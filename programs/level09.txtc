-- # Zero Preservation Initiative
-- # 
-- # Send only the ZEROs to the
-- # OUTBOX.

-- FloorInitNode
  -- register -> floor[1]

-- LetStatementNode
  -- tmp -> floor[0]

-- WhileStatementNode
A:
  -- AssignmentStatementNode
  INBOX
  COPYTO 0 --$tmp
  
  -- IfStatementNode
    -- condition
      -- EqualsConditionNode
      JUMPZ B
    -- else
    C:
      JUMP D
    -- if
    B:
      -- AssignmentStatementNode
      COPYFROM 0 --$tmp
      OUTBOX
  D:
JUMP A