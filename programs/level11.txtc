-- # Sub Hallway
-- # 
-- # For each two things in the
-- # INBOX, first subtract the 1st
-- # from the 2nd and put the
-- # result in the OUTBOX. AND
-- # THEN, subtract the 2nd from
-- # the 1st and put the result in
-- # the OUTBOX. Repeat.
-- # 
-- # You got a new command!
-- # SUBtracts the contents of a tile
-- # on the floor FROM whatever
-- # value you're currently holding.

-- FloorInitNode
  -- register -> floor[2]

-- LetStatementNode
  -- first -> floor[0]

-- LetStatementNode
  -- second -> floor[1]

-- WhileStatementNode
A:
  -- AssignmentStatementNode
  INBOX
  COPYTO 0 --$first
  
  -- AssignmentStatementNode
  INBOX
  COPYTO 1 --$second
  
  -- AssignmentStatementNode
  -- SubtractionExpressionNode
  COPYFROM 0 --$first
  COPYTO 2 --$register
  COPYFROM 1 --$second
  SUB 2 --$register
  OUTBOX
  
  -- AssignmentStatementNode
  -- SubtractionExpressionNode
  COPYFROM 1 --$second
  COPYTO 2 --$register
  COPYFROM 0 --$first
  SUB 2 --$register
  OUTBOX
  
JUMP A