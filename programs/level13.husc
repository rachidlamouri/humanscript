-- # Equalization Room
-- # 
-- # Get two things from the
-- # INBOX. If they are EQUAL, put
-- # ONE of them in the OUTBOX.
-- # Discard non-equal pairs.
-- # Repeat!
-- # 
-- # You got... COMMENTS! You
-- # can use them, if you like, to
-- # mark sections of your
-- # program.

-- FloorInitNode
  -- register -> floor[2]

-- LetStatementNode
  -- first -> floor[0]

-- LetStatementNode
  -- second -> floor[1]

-- WhileStatementNode
A:
  COMMENT 0
  
  -- AssignmentStatementNode
  INBOX
  COPYTO 0 --$first
  
  -- AssignmentStatementNode
  INBOX
  COPYTO 1 --$second
  
  COMMENT 1
  
  -- IfStatementNode
    -- condition
      -- EqualsConditionNode
      COPYFROM 1 --$second
      COPYTO 2 --$register
      COPYFROM 0 --$first
      SUB 2 --$register
      JUMPZ B
    -- else
    C:
      JUMP D
    -- if
    B:
      -- AssignmentStatementNode
      COPYFROM 0 --$first
      OUTBOX
  D:
JUMP A

