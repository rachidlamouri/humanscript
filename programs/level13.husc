-- # Equalization Room
-- #
-- # Get two things from the
-- # INBOX. If they are EQUAL, put
-- # ONE of them in the OUTBOX.
-- # Discard non-equal pairs.
-- # Repeat!
-- #
-- # You got... COMMENTS! You
-- # can use them, if you like, to
-- # mark sections of your
-- # program.

-- FloorInitNode
  -- $accumulator -> floor[2]
  -- $iterator -> null

-- LetStatementNode
  -- first -> floor[0]

-- LetStatementNode
  -- second -> floor[1]

-- WhileStatementNode
JUMP condition00
loop00:
  COMMENT 0 -- ## init

  -- AssignmentStatementNode
    -- read
    INBOX
    -- write
    COPYTO 0 --$first

  -- AssignmentStatementNode
    -- read
    INBOX
    -- write
    COPYTO 1 --$second

  COMMENT 1 -- ## is eq?

  -- IfStatementNode
  -- start01
    -- condition
      -- EqualsConditionNode
      COPYFROM 1 --$second
      COPYTO 2 --$$accumulator
      COPYFROM 0 --$first
      SUB 2 --$$accumulator
      JUMPZ if01
    else01:
      -- block
      JUMP end01
    if01:
      -- block
      -- AssignmentStatementNode
        -- read
        COPYFROM 0 --$first
        -- write
        OUTBOX
  end01:

  condition00:
  JUMP loop00
break00:

