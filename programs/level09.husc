-- # Zero Preservation Initiative
-- # 
-- # Send only the ZEROs to the
-- # OUTBOX.

-- FloorInitNode
  -- accumulator -> floor[1]
  -- iterator -> null

-- LetStatementNode
  -- tmp -> floor[0]

-- WhileStatementNode
JUMP condition00
loop00:
  -- AssignmentStatementNode
    -- read
    INBOX
    -- write
    COPYTO 0 --$tmp
  
  -- IfStatementNode
  -- 01
    -- condition
      -- EqualsConditionNode
      COPYFROM 0 --$tmp
      -- compare 0
      JUMPZ if01
    else01:
      -- block
    JUMP end01
    if01:
      -- block
      -- AssignmentStatementNode
        -- read
        COPYFROM 0 --$tmp
        -- write
        OUTBOX
  end01:
  
  condition00:
  JUMP loop00
break00: