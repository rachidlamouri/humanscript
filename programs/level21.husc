-- # Zero Terminated Sum
-- # 
-- # The INBOX is filled with zero
-- # terminated strings! What's
-- # that? Ask me. Your Boss.
-- # 
-- # Add together all the numbers
-- # in each string. When you reach
-- # the end of a string (marked by
-- # a ZERO), put your sum in the
-- # OUTBOX. Reset and repeat for
-- # each string.

-- FloorInitNode
  -- accumulator -> floor[2]
  -- iterator -> null

-- LetStatementNode
  -- zero -> floor[5]

-- LetStatementNode
  -- sum -> floor[0]

-- LetStatementNode
  -- tmp -> floor[1]

-- AssignmentStatementNode
  -- read
  COPYFROM 5 --$zero
  -- write
  COPYTO 0 --$sum

-- WhileStatementNode
JUMP condition00
loop00:
  -- AssignmentStatementNode
    -- read
    INBOX
    -- write
    COPYTO 1 --$tmp
  
  -- IfStatementNode
  -- 01
    -- condition
      -- NotEqualsConditionNode
      COPYFROM 1 --$tmp
      -- compare 0
      JUMPZ else01
    if01:
      -- block
      -- AssignmentStatementNode
        -- read
        -- AdditionExpressionNode
        COPYFROM 0 --$sum
        COPYTO 2 --$accumulator
        COPYFROM 1 --$tmp
        ADD 2 --$accumulator
        -- write
        COPYTO 0 --$sum
    JUMP end01
    else01:
      -- block
      -- AssignmentStatementNode
        -- read
        COPYFROM 0 --$sum
        -- write
        OUTBOX
      
      -- AssignmentStatementNode
        -- read
        COPYFROM 5 --$zero
        -- write
        COPYTO 0 --$sum
  end01:
  
  condition00:
  JUMP loop00
break00:

