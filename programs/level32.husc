-- # Inventory Report
-- #
-- # For each thing in the INBOX,
-- # send to the OUTBOX the total
-- # number of matching items on
-- # the FLOOR.

-- FloorInitNode
  -- $accumulator -> floor[18]
  -- $iterator -> null

-- LetStatementNode
  -- zero -> floor[14]

-- LetStatementNode
  -- tmp -> floor[15]

-- LetStatementNode
  -- index -> floor[16]

-- LetStatementNode
  -- count -> floor[17]

-- WhileStatementNode
JUMP condition00
loop00:
  COMMENT 0 -- ## init

  -- AssignmentStatementNode
    -- read
    INBOX
    -- write
    COPYTO 15 --$tmp

  -- AssignmentStatementNode
    -- read
    COPYFROM 14 --$zero
    -- write
    COPYTO 16 --$index

  -- AssignmentStatementNode
    -- read
    COPYFROM 14 --$zero
    -- write
    COPYTO 17 --$count

  COMMENT 1 -- ## count

  -- WhileStatementNode
  JUMP condition01
  loop01:
    -- IfStatementNode
    -- 02
      -- condition
        -- EqualsConditionNode
        COPYFROM 15 --$tmp
        COPYTO 18 --$$accumulator
        COPYFROM [16] --$floor[$index]
        SUB 18 --$$accumulator
        JUMPZ if02
      else02:
        -- block
        JUMP end02
      if02:
        -- block
        -- IncrementAssignmentExpressionNode
        BUMPUP 17 --$count
    end02:

    -- IncrementAssignmentExpressionNode
    BUMPUP 16 --$index

    condition01:
    -- NotEqualsConditionNode
    COPYFROM [16] --$floor[$index]
    -- compare 0
    JUMPZ break01
    JUMP loop01
  break01:

  COMMENT 2 -- ## done

  -- AssignmentStatementNode
    -- read
    COPYFROM 17 --$count
    -- write
    OUTBOX

  condition00:
  JUMP loop00
break00:

