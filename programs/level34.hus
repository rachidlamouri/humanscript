# Vowel Incinerator
#
# Send everything from the
# INBOX to the OUTBOX, except
# for vowels.

floor size 10 reserve 0-5

let a at 0
let e at 1
let i at 2
let o at 3
let u at 4
let zero at 5

let tmp

while {
  tmp = inbox

  if (tmp != a && tmp != e && tmp != i && tmp != o && tmp != u) {
     outbox = tmp
  }
}

