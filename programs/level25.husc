-- # Cumulative Countdown
-- # 
-- # For each thing in the INBOX,
-- # OUTBOX the sum of itself plus
-- # all numbers down to zero. For
-- # example, if INBOX is 3,
-- # OUTBOX should be 6, because
-- # 3+2+1 = 6.

-- FloorInitNode
  -- $accumulator -> floor[3]
  -- $iterator -> null

-- LetStatementNode
  -- zero -> floor[5]

-- LetStatementNode
  -- tmp -> floor[0]

-- LetStatementNode
  -- counter -> floor[1]

-- LetStatementNode
  -- sum -> floor[2]

-- WhileStatementNode
JUMP condition00
loop00:
  COMMENT 0 -- ## init
  
  -- AssignmentStatementNode
    -- read
    INBOX
    -- write
    COPYTO 0 --$tmp
  
  -- AssignmentStatementNode
    -- read
    COPYFROM 0 --$tmp
    -- write
    COPYTO 1 --$counter
  
  -- AssignmentStatementNode
    -- read
    COPYFROM 5 --$zero
    -- write
    COPYTO 2 --$sum
  
  COMMENT 1 -- ## sum
  
  -- WhileStatementNode
  JUMP condition01
  loop01:
    -- AssignmentStatementNode
      -- read
      -- AdditionExpressionNode
      COPYFROM 2 --$sum
      COPYTO 3 --$$accumulator
      COPYFROM 1 --$counter
      ADD 3 --$$accumulator
      -- write
      COPYTO 2 --$sum
    
    -- DecrementAssignmentStatementNode
    BUMPDN 1 --$counter
    
    condition01:
    -- GreaterThanConditionNode
    COPYFROM 1 --$counter
    -- compare 0
    JUMPZ break01
    JUMPN break01
    JUMP loop01
  break01:
  
  -- AssignmentStatementNode
    -- read
    COPYFROM 2 --$sum
    -- write
    OUTBOX
  
  condition00:
  JUMP loop00
break00:

